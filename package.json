{
  "name": "react-router-base-template",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "setup": "react-router typegen && playwright install",
    "dev": "react-router dev",
    "build": "react-router build",
    "build:static": "react-router build --mode static",
    "build:ssr": "react-router build --mode ssr",
    "start:ssr": "react-router-serve ./build/server/index.js",
    "lint": "eslint --ignore-pattern .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier . --write --log-level silent",
    "typecheck": "react-router typegen && tsc",
    "typegen:watch": "react-router typegen --watch",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "knip": "knip",
    "validate": "run-p test lint typecheck test:e2e",
    "prepare": "husky"
  },
  "dependencies": {
    "@react-router/node": "7.0.0-pre.5",
    "@react-router/serve": "7.0.0-pre.5",
    "isbot": "^5.1.17",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "7.0.0-pre.5"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.2",
    "@eslint/js": "^9.14.0",
    "@playwright/test": "^1.48.2",
    "@react-router/dev": "7.0.0-pre.5",
    "@storybook/addon-essentials": "^8.4.2",
    "@storybook/addon-interactions": "^8.4.2",
    "@storybook/react": "^8.4.2",
    "@storybook/react-vite": "^8.4.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitest/coverage-v8": "^2.1.4",
    "@vitest/eslint-plugin": "^1.1.10",
    "dotenv": "^16.4.5",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^3.9.1",
    "eslint-plugin-playwright": "^2.0.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-storybook": "^0.11.0",
    "eslint-plugin-testing-library": "^6.4.0",
    "eslint-plugin-unicorn": "^56.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.12.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "knip": "^5.36.7",
    "lint-staged": "^15.2.10",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.48.2",
    "prettier": "^3.3.3",
    "storybook": "^8.4.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.2",
    "vitest": "^2.1.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown --log-level silent"
  },
  "engines": {
    "node": ">=20.18.0"
  },
  "volta": {
    "node": "20.18.0"
  }
}
