{
  "name": "react-router-base-template",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "build": "react-router build",
    "build:static": "react-router build --mode static",
    "build:ssr": "react-router build --mode ssr",
    "start:ssr": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "typegen:watch": "react-router typegen --watch",
    "postinstall": "react-router typegen"
  },
  "dependencies": {
    "@react-router/node": "7.0.0-pre.5",
    "@react-router/serve": "7.0.0-pre.5",
    "isbot": "^5.1.17",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "7.0.0-pre.5"
  },
  "devDependencies": {
    "@react-router/dev": "7.0.0-pre.5",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-tsconfig-paths": "^5.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "volta": {
    "node": "20.18.0"
  }
}
